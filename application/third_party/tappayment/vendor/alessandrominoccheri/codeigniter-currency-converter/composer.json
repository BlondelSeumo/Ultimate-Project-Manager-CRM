{
    "name": "alessandrominoccheri/codeigniter-currency-converter",
    "type": "plugin",
    "description": "A Codeigniter library to convert your price from a currency to another",
    "keywords": ["codeigniter","currency", "price", "convert"],
    "homepage": "https://github.com/AlessandroMinoccheri/codeigniter-currency-converter",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Minoccheri",
            "homepage": "http://alessandrominoccheri.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "codeigniter/framework": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "": [
                    "libraries/",
                    "tests/"
                ]
        }
